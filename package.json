    "eslint": "^6.6.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "expect.js": "^0.3.1"
  },
  "dependencies": {},
  "mocha": {
    "spec": "lib/**/*.test.js"
  }
